<?xml version="1.0"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!-- Use URL for BrowserStack capability information https://www.browserstack.com/automate/java -->
<BrowserStack>
    <Environment Name="Windows">
        <Browser>IE</Browser>
        <BrowserVersion>8.0</BrowserVersion>
        <OperatingSystem>Windows</OperatingSystem>
        <OperatingSystemVersion>7</OperatingSystemVersion>
        <Resolution>1024x768</Resolution>
    </Environment>
    <Environment Name="MobileAndroid">
        <Browser>android</Browser>
        <OperatingSystem>ANDROID</OperatingSystem>
        <Device>HTC One M8</Device>
    </Environment>
    <Environment Name="MobileIOS">
        <Browser>iPhone</Browser>
        <OperatingSystem>MAC</OperatingSystem>
        <Device>iPhone 6S Plus</Device>
    </Environment>
    
    
</BrowserStack>